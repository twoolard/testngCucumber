<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Cucumber Regression Test" verbose="1" thread-count="2" parallel="tests" configfailurepolicy="continue">
    <listeners>
        <listener class-name="utils.listeners.TestNGExecutionListener">
        </listener>
    </listeners>
    <test name="Run Smoke Test" annotations="JDK" preserve-order="true">
        <classes>
            <class name="com.company.runners.RegressionTest"/>
        </classes>
    </test>
</suite>